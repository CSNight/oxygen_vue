{
  "name": "redis_oxygen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "dependencies": {
    "@riophae/vue-treeselect": "0.4.0",
    "axios": "^0.21.0",
    "brace": "0.11.1",
    "echarts": "^4.9.0",
    "element-ui": "^2.14.0",
    "js-cookie": "2.2.1",
    "mavon-editor": "^2.9.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "^6.2.0",
    "svg-sprite-loader": "^5.0.0",
    "vue": "2.6.12",
    "vue-cron-generator": "0.1.9",
    "vue-i18n": "^8.22.1",
    "vue-infinite-loading": "2.4.5",
    "vue-router": "3.4.9",
    "vue2-editor": "2.10.2",
    "vue2-transitions": "0.3.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.8",
    "@vue/cli-plugin-eslint": "4.5.8",
    "@vue/cli-service": "4.5.8",
    "babel-eslint": "10.1.0",
    "compression-webpack-plugin": "^6.0.5",
    "eslint": "7.13.0",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "4.5.0",
    "node-sass": "5.0.0",
    "sass-loader": "10.0.5",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "script-loader": "0.7.2",
    "serve-static": "^1.14.1",
    "svgo": "1.3.2",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": false
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": 1
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
